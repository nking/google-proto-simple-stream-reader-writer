<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
   
    <application>DemoGPB</application>
    <version>1</version>

    <ssl-enabled>false</ssl-enabled>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>

    <precompilation-enabled>false</precompilation-enabled>

    <static-files>
        <include path="testGPB.html" />
        
        <include path="/js/protobuf.js" />
        <include path="/js/pbj.js" />
        <include path="/js/examplemessage.proto.js" />
        
        <include path="/js/googleProtocolBufferMessagesReader.js" />
        
        <include path="/js/jquery-1.6.4.min.js" />
        <include path="/js/qunit.js" />
        <include path="/stylesheets/qunit.css"/>
    </static-files>
</appengine-web-app>
